{
  "name": "readafull-infrastructure",
  "version": "0.1.0",
  "private": true,
  "description": "AWS CDK infrastructure for Readafull mobile application",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:deploy": "cdk deploy --all",
    "cdk:deploy:dev": "cdk deploy --all --context environment=development",
    "cdk:deploy:staging": "cdk deploy --all --context environment=staging",
    "cdk:deploy:prod": "cdk deploy --all --context environment=production",
    "cdk:destroy": "cdk destroy --all",
    "cdk:diff": "cdk diff",
    "cdk:synth": "cdk synth"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "aws-cdk": "^2.133.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.133.0",
    "constructs": "^10.3.0"
  }
}
